
@{
    
    int H_W = Request.Browser.ScreenPixelsHeight + 24;

}


<!DOCTYPE html>
<html lang="en">

@Html.Partial("_Head")

<body class="no-skin" id="super_parent">
    <div id="navbar" class="navbar navbar-default ace-save-state navbar-fixed-top">
        <div class="navbar-container ace-save-state" id="navbar-container">
            <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler" data-target="#sidebar">
                <span class="sr-only">Toggle sidebar</span>

                <span class="icon-bar"></span>

                <span class="icon-bar"></span>

                <span class="icon-bar"></span>
            </button>

            <div class="navbar-header pull-left" id="nav-ptpr">
                <div class="navbar-brand" style="padding:0px;padding-top:8px;padding-left:2px;text-align:center;">
                </div>
                <div class="navbar-brand">
                    <small>Test <span style="font-size:11px;">(v.1.1)</span></small>
                </div>
            </div>

            <div class="navbar-buttons navbar-header pull-right" role="navigation">
                <ul class="nav ace-nav">
                    <li class="light-green dropdown-modal">
                        <a data-toggle="dropdown" href="#" class="dropdown-toggle">

                            <span class="user-info">
                                <small>Welcome,</small>
                                @User.Identity.Name
                            </span>

                            <i class="ace-icon fa fa-caret-down"></i>
                        </a>

                        <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">


                            <li>
                                <a href="#" onclick="DetailUser()">
                                    <i class="ace-icon fa fa-user"></i>
                                    Profile
                                </a>
                            </li>

                            <li class="divider"></li>

                            <li>
                                <a href="@Url.Content("~/User/Logout")">
                                    <i class="ace-icon fa fa-power-off"></i>
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div><!-- /.navbar-container -->
    </div>

    <div class="main-container ace-save-state " id="main-container">
        <script type="text/javascript">
            try { ace.settings.loadState('main-container') } catch (e) { }
        </script>


        @if (ViewBag.TopMenuAce == "Yes")
        {

            @Html.Partial("_SideMenu")

        }

        <div class="main-content">
            <div class="main-content-inner">
                <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                    <ul class="breadcrumb">
                        <li>
                            @Html.Raw(ViewBag.Icon + " " + ViewBag.Parent_Caption)
                        </li>
                        @if (ViewBag.Child_Caption != null)
                        {
                            <li class="active">@Html.Raw(ViewBag.Child_Caption)</li>
                        }
                        @if (ViewBag.SubChild != null)
                        {
                            <li class="active">@ViewBag.SubChild</li>
                        }

                    </ul><!-- /.breadcrumb -->

                </div>

                <div class="">


                    @RenderBody()



                </div><!-- /.page-content -->



            </div>
        </div><!-- /.main-content -->

        <div class="footer">
            <div class="footer-inner">
                <div class="footer-content">
                    <span class="bigger-120">
                        <span style="color:#5D8AA5;font-size:13px;">Rizkypprtm &copy; 2022</span>
                    </span>


                </div>
            </div>
        </div>

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
    </div><!-- /.main-container -->


    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='../../Content/assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
    </script>
    <![endif]-->




    <!-- Modal Filter -->
    <div id="Modal_load" class="modal" role="dialog">

        <div class="modal-dialog" style="text-align: center;padding-top:8%;">

            <img src="~/Content/images/loading_images/bee_loading.gif" style="width:60%;" />
            <br /><span style="font-size:20px;">Loading ...</span>

        </div>

    </div>


    <!-- Modal Filter -->
    <div id="Modal_load_2" class="modal" role="dialog">

        <div class="modal-dialog" style="text-align: center;padding-top:8%;">

            <img src="~/Content/images/loading_images/cricle.gif" style="width:60%;" />
            <br /><span style="font-size:20px;">Loading ...</span>

        </div>

    </div>


    <!-- Modal Filter -->
    <div id="Saving_Load" class="modal" role="dialog">

        <div class="modal-dialog">

            <img src="~/Content/images/loading_images/cloud-upload.gif" style="width:200px;" />
            <br /><span style="font-size:13px;">Please Wait ...</span>

        </div>

    </div>

    <!-- Modal Filter -->
    <div id="Deleting_Load" class="modal" role="dialog">

        <div class="modal-dialog">

            <img src="~/Content/images/loading_images/deleting.gif" style="width:200px;" />
            <br /><span style="font-size:13px;">Please Wait ...</span>

        </div>

    </div>

    <!-- Modal Filter -->
    <div id="Uploading_Load" class="modal" role="dialog">

        <div class="modal-dialog" style="text-align: center;padding-top:15%;">

            <img src="~/Content/images/loading_images/uploading.gif" style="width:25%;" />
            <br /><span style="font-size:13px;">Uploading File On Progress ...</span>

        </div>

    </div>

    <!-- Modal Filter -->
    <div id="Modal_Info" class="modal" role="dialog">
        <div class="modal-dialog" style="text-align: center;padding-top:10%;">
            <span style="color:white;position:absolute;left:39.5%;" class="info-wait">Please Wait ...</span>
        </div>

    </div>

    <!-- Modal -->

    

    <div id="myModal_Unit" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">List Unit</h4>
                </div>
                <div class="modal-body" id="tab-unit-list" style="padding:0px;">

                    <div id="PopUpUnit" style="height:300px;border-left:none;border-bottom:1px dotted #dedede;border-radius:0px;" class="responsive"></div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-info" onclick="select_unit_1()">select</button>
                    <button type="button" class="btn btn-info" data-dismiss="modal">Cancel</button>
                </div>
            </div>

        </div>
    </div>


</body>



<script>
    
	$('.select2').css('width', '100%').select2()
</script>
</html>




